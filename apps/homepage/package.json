{
    "name": "@isptutorproject/homepage",
    "version": "0.1.0",
    "description": "isp tutor project home page",
    "main": "src/index.js",
    "license": "UNLICENSED",
    "private": true,
    "scripts": {
        "build": "npm run setup && rimraf dist && NODE_ENV=production DEBUG=false  webpack --mode=production && eleventy",
        "watch": "npm run setup && rimraf dist && NODE_ENV=development DEBUG=true webpack --watch --mode=development & eleventy --watch",
        "setup": "cd templates/_includes && ln -sfn ../../../../config/eleventy-config/sitewide_includes site && cd ../.."
    },
    "dependencies": {
        "@isptutorproject/isp-database": "0.1.0",
        "@isptutorproject/isp-site-styles": "0.1.0"
    },
    "devDependencies": {
        "@11ty/eleventy": "0.10.0",
        "@isptutorproject/eleventy-config": "0.1.0",
        "@isptutorproject/webpack-config": "0.1.0",
        "rimraf": "^3.0.2",
        "webpack": "4.42.0",
        "webpack-cli": "3.3.11"
    }
}