{
    "name": "@isptutorproject/homepage",
    "version": "0.1.0",
    "description": "isp tutor project home page",
    "main": "src/index.js",
    "license": "UNLICENSED",
    "private": true,
    "scripts": {
        "build": "rimraf dist && NODE_ENV=production DEBUG=false  webpack --mode=production",
        "watch": "rimraf dist && NODE_ENV=development DEBUG=true webpack --watch --mode=development",
        "setup": "cd templates/_includes && ln -sfn ../../../../config/eleventy-config/sitewide_includes site && cd ../.. && cd templates/img && mkdir -p site && cp -r ../../../../config/eleventy-config/sitewide_images/* site && cd ../.."
    },
    "dependencies": {
        "@isptutorproject/isp-database": "0.1.0",
        "@isptutorproject/isp-site-styles": "0.1.0",
        "@isptutorproject/navbar": "0.1.0",
        "@isptutorproject/snackbar": "0.1.0"
    },
    "devDependencies": {
        "@isptutorproject/webpack-config": "0.1.0",
        "rimraf": "^3.0.2",
        "sass": "^1.26.5",
        "sass-loader": "^8.0.2",
        "webpack": "4.42.0",
        "webpack-cli": "3.3.11"
    }
}
