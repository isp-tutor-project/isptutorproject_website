<div class="intro-container dev">
    <div class="intro-left dev">
        {% if teacher == "odd" %}
            <img width="196" src="/img/sf-assessment/teacherEven.png"  />
        {% else %}
            <img width="196" src="/img/sf-assessment/teacherOdd.png"  />
        {% endif %}
